{% extends 'admin/base.html.twig' %}

{% block title %}
{% endblock %}

{% block main %}
    <section>
        <div class="container">
            <div class="row">
                <div class="col-12">
                    {% for admin in admins %}
                        <h2>{{ admin.email }}</h2>
                        {% for role in admin.roles %}
                            <span>{{ role }} | </span>
                        {% endfor %}
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>
{% endblock %}